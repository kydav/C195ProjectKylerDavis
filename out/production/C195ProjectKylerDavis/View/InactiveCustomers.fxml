            <TableColumn fx:id="phoneColumn" prefWidth="91.0" text="%generic.phone" />
            <TableColumn fx:id="cityColumn" prefWidth="101.0" text="%generic.city" />
            <TableColumn fx:id="postalCodeColumn" prefWidth="98.0" text="%generic.postalCode" />
