      <ButtonBar layoutX="255.0" layoutY="388.0" prefHeight="40.0" prefWidth="322.0">
         <buttons>
            <Button fx:id="editInactive" defaultButton="true" mnemonicParsing="false" onAction="#editInactive" text="%manage.editButton" />
            <Button fx:id="cancelButton" cancelButton="true" mnemonicParsing="false" onAction="#cancelInactive" text="%manage.cancelButton" />
         </buttons>
      </ButtonBar>
      <Text layoutX="221.0" layoutY="37.0" strokeType="OUTSIDE" strokeWidth="0.0" text="%inactive.title" textAlignment="CENTER">
