<AnchorPane prefHeight="442.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller.InactiveCustomersController">
   <children>
      <TableView fx:id="inactiveCustomerTableView" layoutX="15.0" layoutY="49.0" prefHeight="339.0" prefWidth="571.0">
         <columns>
            <TableColumn fx:id="idColumn" prefWidth="37.0" text="ID" />
