<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller.ManageCustomerController">
   <children>
      <TableView fx:id="manageCustomerTableView" layoutX="14.0" layoutY="15.0" prefHeight="339.0" prefWidth="571.0">
         <columns>
            <TableColumn fx:id="idColumn" prefWidth="37.0" text="ID" />
            <TableColumn fx:id="nameColumn" prefWidth="132.0" text="Name" />
            <TableColumn fx:id="phoneColumn" prefWidth="91.0" text="Phone" />
            <TableColumn fx:id="cityColumn" prefWidth="101.0" text="City" />
            <TableColumn fx:id="postalCodeColumn" prefWidth="98.0" text="Postal Code" />
            <TableColumn fx:id="countryColumn" prefWidth="111.0" text="Country" />
         </columns></TableView>
      <ButtonBar layoutX="254.0" layoutY="354.0" prefHeight="40.0" prefWidth="322.0">
         <buttons>
            <Button fx:id="manageCustomerEdit" defaultButton="true" mnemonicParsing="false" onAction="#editCustomer" text="%manage.editButton" />
            <Button fx:id="manageCustomerDelete" defaultButton="true" mnemonicParsing="false" onAction="#deleteCustomer" text="%manage.deleteButton" />
            <Button fx:id="manageCustomerNew" defaultButton="true" mnemonicParsing="false" onAction="#newCustomer" text="%manage.newButton" />
            <Button fx:id="manageCustomerCancel" cancelButton="true" mnemonicParsing="false" onAction="#cancelManageCustomer" text="%manage.cancelButton" />
         </buttons>
      </ButtonBar>
   </children>
</AnchorPane>
